public class BankAccount {
	private String name, accountNo;
	private int balance;
	private double rate;
	
	public String toString ()
	{
		String rslt = "";
		rslt += ("이름 : " + name + "\n");
		rslt += ("계좌 번호 : " + accountNo + "\n");
		rslt += ("잔액 : " + balance + "원\n");
		rslt += ("이자율 : " + rate + "%\n");
		rslt += ("이자 : " + calcInterest() + "원\n");
		return rslt;
	}
	
	private int calcInterst() 
	{
		return (int)rate * balance;
	}
}
