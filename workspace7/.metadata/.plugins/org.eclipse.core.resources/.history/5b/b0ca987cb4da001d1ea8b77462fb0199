import java.util.Scanner;
public class Practice55 {

	public static void main(String[] args) {
		Date birthday = new Date();
		
		Scanner sc = new Scanner(System.in);
		
		Date d1 = new Date();
		System.out.println("첫 번째 날짜를 입력하세요.");
		System.out.print("년도 : ");
		d1.setYear(sc.nextInt());
		System.out.print("월 : ");
		d1.setMonth(sc.nextInt());
		System.out.print("일 : ");
		d1.setDay(sc.nextInt());
		
		Date d2 = new Date(sc.nextInt(),sc.nextInt(),sc.nextInt());
		System.out.println("두 번째 날짜를 입력하세요.");
		System.out.print("년도 : ");
		birthday.setYear(sc.nextInt());
		System.out.print("월 : ");
		birthday.setMonth(sc.nextInt());
		System.out.print("일 : ");
		birthday.setDay(sc.nextInt());
		sc.close();
		
		
	}

}
