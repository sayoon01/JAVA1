import java.util.Scanner;

public class Practice58 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		Shape s = new Shape();
		System.out.println("Shape 썂칲 촋줮쟍 첎췒핖. ");
		System.out.print("x촥킨 : ");
		s.x = sc.nextInt();
		System.out.print("y촥킨 : ");
		s.y = sc.nextInt();
		
		Circle c1 = new Circle();
		System.out.println("Circle 썂칲 촋줮쟍 첎췒핖. ");
		System.out.print("x촥킨 : ");
		c1.x = sc.nextInt();
		System.out.print("y촥킨 : ");
		c1.y = sc.nextInt();
	}

}
