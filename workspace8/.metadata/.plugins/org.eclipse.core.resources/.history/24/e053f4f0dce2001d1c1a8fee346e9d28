
public class Rectangle extends Shape{
	private int width,height;
	
	public Rectangle() {
		super(); //명시적인 호출
	}
	
	public Rectangle(int width, int height) {
		this.width = width;
		this.height = height;
	}
	
	public void printInfo()
	{
		System.out.println("사각형 :: 중심좌표  " + getX() + "," + getY() + ") :: 면적 - " + width*height + " :: 둘레 - " + (height*2 + width*2));  
	}
}
