package nofruit;

import fruit.*;

public class Practice67 {

	public static void main(String[] args) {
		System.out.println("읍식 정보입니다.");
		Food f0 = new Food();
		f0.setCalory(100);
		f0.setPrice(100);
		f0.setWeight(12.3);
		System.out.print(f0.toString());
		
		System.out.println("\n멜론의 정보 입니다.");
		Melon f0 = new Food();
		f0.setCalory(100);
		f0.setPrice(100);
		f0.setWeight(12.3);
		System.out.print(f0.toString());
	}

}
