
public class Undergraduate extends Student{
	private int year;

	public int getYear() {
		return year;
	}

	public void setYear(int year) {
		this.year = year;
	}

	public String toString() {
		return "학번 : " + getId() + ", 등록금 : " + getTuition() + ", 평균등급 : " + getGpa() + " 학년 : " + year + "";
	}
	
	public Undergraduate() {
		year = 0;
	}
}
